{
    "name": "react4xp-runtime-client",
    "version": "0.1.1",
    "description": "React4xp helper: Webpack setup for building React4xp's client-side wrapper",
    "main": "webpack.config.js",
    "files": [
        "react4xpClient.es6",
        "webpack.config.js",
        "README.md"
    ],
    "scripts": {
        "clean": "npm run cleanRaw && npm run cleanWebpack",
        "cleanRaw": "rimraf ./react4xpClient.es6",
        "cleanWebpack": "rimraf ./webpack.config.js",
        "copy": "copyfiles --flat src/*.* .",
        "prepublish": "npm run clean && npm run copy",
        "postpublish": "npm run clean"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/enonic/react4xp-runtime-client"
    },
    "author": "Espen Norderud",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/enonic/react4xp-runtime-client/issues"
    },
    "homepage": "https://github.com/enonic/react4xp-runtime-client",
    "dependencies": {
        "chunks-2-json-webpack-plugin": "^1.0.0"
    },
    "peerDependencies": {
        "babel-cli": "^6.0.0",
        "babel-core": "^6.0.0",
        "babel-eslint": "^7.0.0",
        "babel-loader": "^7.1.5",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "react-dom": "^16.0.0",
        "webpack": "^4.29.0",
        "webpack-cli": "^3.2.0"
    },
    "devDependencies": {
        "copyfiles": "^2.0.0",
        "rimraf": "^2.0.0"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-0"
        ],
        "plugins": [
            "transform-object-rest-spread"
        ]
    }
}
